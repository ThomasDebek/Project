    $('.timer').countTo();                // I tutaj dajmy nasze odliczanie  z naszeg pluginu - podpinam sie do klasy timer i wywoluje funkcji countTo
    /* viewport */


    /* YTPlayer */
    $('.player').YTPlayer();      //  Musze sie odwolac do tego elementu do ktorego bedzie wywylywany nasz player


    // Tu mam pogladowy skrypt

    $('.our-team').viewportChecker({      // Zwracamy uwage na wielkosc liter   Tutaj bedziemy mieli nasza klase
      classToAdd: 'animated slideInLeft'        // Tutaj dodamy nasza animiacjie (opcja ktora nas interesuje powinna dodawac klase  ( tutaj znajduje sie lista dostepnych opcji: https://github.com/dirkgroenen/jQuery-viewport-checker)
    });                                   // Nas interesuje jedna czyli classToAdd: oraz element ktory wygeneruje nam animacje
                                          // animated - to klasa dodana w naszym laytsie
                                          // a swing - to dostepna animacja z strony : https://daneden.github.io/animate.css/
                                          // CZYLI - TE DWIE KLASY ZOSTANA DODANE DO NASZEJ KLASY ALE DOPIERO WTEDY KIEDY ZOSTANIE ZALOADOWANY DO VIEWPORTU

/*
    $('.opinions').viewportChecker({           // Fack - ale zajebioza
      classToAdd: 'animated slideInRight'
    });
*/

    /* viewport */
    $('*[data-animate]').addClass('hide').each(function(){            // Za pomocą jQery odwolam sie do wszystkich elementow data-animate na stronie i dodam do niej domieszke hide-ukryj-domieszka i przeliteruje petlą po nich
      $(this).viewportChecker({                                       // na kazdym elemencie (this) wywolam nasz viewportChecker
        classToAdd: 'show animated ' + $(this).data('animate'),       // i na kazdym elemencie spelniajacym nasze kryterium dodamy klse-domieszke-show + oprocz niej musze dodac nasza animiacjie
        classToRemove: 'hide',                                        // i WAZENE musimy takze usunac klase
        offset: 200                                                   // oraz dodam opuznienie 1/5sekundy
      });
    });
  });




